    Algoritmo NumeroMayoR
		Definir N1, N2 Como Entero;
		Escribir "Ingresa el valor del primer numero";
		Leer N1;
		Escribir "Ingresa el valor de segundo numero";
		Leer N2;
		Si   N1>N2 Entonces
			Escribir "El numero mayor es:",N1;
		SiNo
        Escribir "El numero mayor es:",N2
	FinSi
	
FinAlgoritmo
